!-------------------------------------------------!
!vim:fenc=UTF-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults!
!-------------------------------------------------!

! Xft setting
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    full
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! rxvt-unicode settings
URxvt.transparent:     false
URxvt.imLocale:        en_US.UTF-8
URxvt.scrollBar:       false
URxvt.saveLines:       32767
URxvt.cursorBlink:     true
URxvt.cursorColor:     #FF0000
URxvt.visualBall:      true
URxvt*termName:        rxvt-unicode-256color
URxvt*font:            xft:monaco:pixelsize=12:antialias=true:hinting=true
URxvt*boldFont:        xft:monaco:pixelsize=12:antialias=true:hinting=true:bold
URxvt*italicFont:      xft:monaco:pixelsize=12:antialias=true:hinting=true:italic
URxvt*bolditalicFont:  xft:monaco:pixelsize=12:antialias=true:hinting=true:bold:italic
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher:     google-chrome
URxvt.matcher.button:  1
URxvt*urgentOnBell:    true
URxvt*iso14755:        false

! seoul256 (dark) theme adapted for URxvt
URxvt*background: #3a3a3a
URxvt*foreground: #d0d0d0
URxvt*color0: #4e4e4e
URxvt*color1: #d68787
URxvt*color2: #5f865f
URxvt*color3: #d8af5f
URxvt*color4: #85add4
URxvt*color5: #d7afaf
URxvt*color6: #87afaf
URxvt*color7: #d0d0d0
URxvt*color8: #626262
URxvt*color9: #d75f87
URxvt*color10: #87af87
URxvt*color11: #ffd787
URxvt*color12: #add4fb
URxvt*color13: #ffafaf
URxvt*color14: #87d7d7
URxvt*color15: #e4e4e4
URxvt*cursorColor: #d0d0d0
URxvt*cursorColor2: #3a3a3a
URxvt*colorBD: #e4e4e4
