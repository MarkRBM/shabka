!---------------------------------------------- file: .Xdefaults author: Chris
!Borrrell http://borrellstudios.com/scripts/.Xdefaults modified: 24 Dec 09
!vim:fenc=UTF-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
!----------------------------------------------

Xft.dpi:		    96
Xft.antialias:		true
Xft.hinting:		full
Xft.hintstyle:		hintslight
Xft.rgba:		    rgb

URxvt.transparent:	true
URxvt.inheritPixmap:true
URxvt.imLocale:		en_AU.UTF-8
URxvt.scrollBar:	false
URxvt.saveLines:	32767
URxvt.cursorBlink:	true
URxvt.cursorColor:	#FFFFFF
URxvt.color:		#FFFFFF
URxvt.shading:		30
URxvt.tintcolor:	#151515
URxvt.depth:		24
URxvt.visualBall:	true

URxvt*font:		        xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true
URxvt*boldFont:		    xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true:bold
URxvt*italicFont:	    xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true:italic
URxvt*bolditalicFont:	xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true:bold:italic

URxvt*perl-ext-common:  default,matcher,clipboard
URxvt*urlLauncher:      google-chrome
URxvt.matcher.button:   1


