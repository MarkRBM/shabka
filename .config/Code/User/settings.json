{
    "go.toolsGopath": "~/.filesystem",
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": ",",
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.sneak": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.otherModesKeyBindings": [
        {
            "before": ["n"],
            "after": ["<left>"]
        },
        {
            "before": ["e"],
            "after": ["<down>"]
        },
        {
            "before": ["i"],
            "after": ["<up>"]
        },
        {
            "before": ["o"],
            "after": ["<right>"]
        }
    ],
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": ["s"],
            "after": ["i" ]
        },
        {
            "before": ["<leader>", "w", "w"],
            "after": [],
            "commands": [
                {
                    "command": ":wall"
                }
            ]
        },
        {
            "before": ["h"],
            "after": ["o"]
        },
        {
            "before": ["H"],
            "after": ["O"]
        },
        {
            "before": ["<c-j>"],
            "after": ["<c-n>"]
        },
        {
            "before": ["k"],
            "after": ["n"]
        },
        {
            "before": ["K"],
            "after": ["N"]
        },
        {
            "before": ["u"],
            "after": ["i"]
        },
        {
            "before": ["U"],
            "after": ["I"]
        },
        {
            "before": ["l"],
            "after": ["u"]
        },
        {
            "before": ["L"],
            "after": ["U"]
        },
        {
            "before": ["N"],
            "after": ["J"]
        },
        {
            "before": ["E"],
            "after": ["K"]
        },
        {
            "before": ["I"],
            "after": ["L"]
        },
        {
            "before": ["j"],
            "after": ["e"]
        },
        {
            "before": ["J"],
            "after": ["E"]
        }
    ]
}
