  source $HOME/.screen/config

# Overwrite default escape
  escape ^zz

# Overwrite default shell.
  shell -zsh

# Default Buffers
#               <Title>         <Screen Number>     <Command>
  screen    -t  SshForward      0                   ssh -4 gaea-ub-for-port-forwarding -L 6667:gaea-ub.dls.corp.google.com:6667 -N

  sleep 30

  screen    -t  Irssi           1                   irssi
  select 1
